# Marzban Configuration Example
# Copy this file to .env and fill in your values

# Telegram Bot Token (from @BotFather)
TELEGRAM_TOKEN=your_bot_token_here

# Admin Telegram ID (for bot access)
TELEGRAM_ADMIN_IDS=12345678,98765432

# Database URL (SQLite is default)
SQLALCHEMY_DATABASE_URL=sqlite:///db.sqlite3

# Xray Executable Path
XRAY_EXECUTABLE_PATH=/var/lib/marzban/xray-core/xray

# Language (set to ru for Russian)
LOCALE=ru

# Marzban Dashboard Access
UVICORN_HOST=0.0.0.0
UVICORN_PORT=8000
SUDO_USERNAME=admin
SUDO_PASSWORD=your_secure_password
